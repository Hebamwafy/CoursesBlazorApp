@using AKSoftware.Blazor.Utilities
< div class="alert alert-success" role="alert">
    @Name Has Been Added Successfully.
    <h2>Press Ok To Add New Course</h2>
    < @onclick="OK" type="button" class="btn btn-dark"> OK </ button>
</ div>

@code {
    [Parameter] public string Name { set; get; } = string.Empty;
    void OK()
    {
        MessagingCenter.Send(this, "ShowFormMsg");
    }
}
